{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": ["regexSplit"],
      "markdownDescription": "Human-readable label that identifies this tokenizer type. Value must be `regexSplit`."
    },
    "pattern": {
      "type": "string",
      "description": "Regular expression to match against"
    }
  },
  "required": ["type","pattern"],
  "additionalProperties": false
}