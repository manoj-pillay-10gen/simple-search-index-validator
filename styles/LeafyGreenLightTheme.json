{
  "base": "vs",
  "inherit": true,
  "rules": [
    {
      "token": "comment",
      "foreground": "#007AB8",
      "fontStyle": "italic"
    },
    {
      "token": "keyword.json",
      "foreground": "#F2590C"
    },
    {
      "token": "string.key.json",
      "foreground": "#016EE9"
    },
    {
      "token": "string.value.json",
      "foreground": "#12824D"
    },
    {
      "token": "number.value.json",
      "foreground": "#D83713"
    },
    {
      "token": "attribute.value.number",
      "foreground": "#D83713"
    },
    {
      "token": "number",
      "foreground": "#D83713"
    }
  ],
  "colors": {
    "editor.background": "#F9FBFA",
    "editor.foreground": "#3D4F58",
    "editor.lineHighlightBackground": "#FDF7E5",
    "editor.selectionBackground": "#FFFFFF",
    "editor.selectionHighlightBackground": "#FFFFAD",
    "editor.snippetTabstopHighlightBackground": "#E1FEFF",
    "editorCursor.foreground": "#000000",
    "editorIndentGuide.background": "#FFFFFF",
    "editorWhitespace.foreground": "#BFBFBF"
  }
}
